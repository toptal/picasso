{
  "name": "@toptal/ui-library",
  "version": "0.1.0-alpha.0",
  "description": "",
  "main": "build/index.js",
  "module": "build/es/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "echo \"Error: no lint specified\" && exit 1",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "release:alpha": "yarn version --new-version prerelease",
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel --config-file ./babel.config.js ./components --out-dir ./build --ignore 'components/**/test.js,components/**/test.jsx'",
    "build:es6": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./components --out-dir ./build/es --ignore 'components/**/test.js,components/**/test.jsx'",
    "build": "yarn prebuild && yarn build:es2015 && yarn build:es6",
    "postinstall": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toptal/ui-library.git"
  },
  "author": "Toptal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toptal/ui-library/issues"
  },
  "homepage": "https://github.com/toptal/ui-library#readme",
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@storybook/react": "^4.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "react-testing-library": "^5.3.1",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.2",
    "rollup-plugin-node-resolve": "^3.4.0"
  }
}
